{
  "name": "light-show",
  "version": "0.1.7",
  "description": "Contains a lightweight document editor and reader.",
  "author": "XinZhu",
  "main": "./lib/light-show.common.js",
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name light-show --dest lib packages/index.js",
    "package": "vue-cli-service build --target lib --name light-show --dest lib ./components/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  
  "homepage": "https://github.com/Cxz-web/light-show",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cxz-web/light-show"
  },
  
  "keywords": [
    "vue",
    "component",
    "slide"
  ],
  
  "bugs": {
    "url": "https://github.com/Cxz-web/light-show/issues"
  },
  
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
